{
  "name": "docker-demo-showcase",
  "version": "2.0.0",
  "description": "Docker features demo showcasing Docker Hub, Build Cloud, and Scout",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen",
    "test:all": "npm run lint && npm run test && npm run test:e2e",
    "docker:build": "docker buildx build -t murr2k/docker-demo-showcase .",
    "docker:scout": "docker scout cves murr2k/docker-demo-showcase"
